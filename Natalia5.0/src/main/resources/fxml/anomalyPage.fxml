   <children>
      <LineChart fx:id="priceChart" layoutX="10.0" prefHeight="338.0" prefWidth="580.0" stylesheets="@../style/anomalyPage.css">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <Button fx:id="loadDataButt" layoutX="274.0" layoutY="338.0" mnemonicParsing="false" onAction="#loadData" text="Load data" />
